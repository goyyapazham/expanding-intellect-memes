import base64


#Converting image to string
with open("randImg.png","rb") as imageFile:
    str = base64.b64encode(imageFile.read())
    print str




#Converting string to image
fh = open("imageToSave.png","wb")
fh.write(str.decode('base64'))
fh.close()


